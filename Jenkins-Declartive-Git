pipeline{
    agent any
    
    environment{
            PATH = "/opt/maven-3.3.9/bin:$PATH" 
    }
    stages{
        stage("Git Checkout"){
            steps{
                git credentialsId: 'be0bd67a-5e67-4c09-bbc2-7d013599cc0d', url: 'https://github.com/shantanu605/myweb.git'
            }
        }
        stage{
         step("Maven Build"){
          steps{
              sh "mvn clean package"
              }
          }
        }
    }
}
